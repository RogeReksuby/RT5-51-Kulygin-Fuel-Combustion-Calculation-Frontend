{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default permissions for the app",
  "windows": ["main"],
  "permissions": [
    "core:default",
    {
      "identifier": "http:default",
      "allow": [
        { "url": "http://192.168.1.172:8080/*" },
        { "url": "http://192.168.1.172:9000/*" }
      ]
    }
  ]
}